[{
  "id_usuario": "{{ id_usuario }}",
  "nome": "{{ nome }}",
  "exibirTodasOpcoesEstr": "{{ exibirTodasOpcoesEstr }}",
  "exibirTG": "{{exibirTG}}",
  "dataHoraInicio": "{{ dataHoraInicio }}",
  "opcoes":
      [
        {
          "id": "",
          "rotulo": "---------"
        },
        {% for e in estruturas %}
          {
            "id": "{{e.id}}",
            "rotulo": "MO: {{ e.ordemproducao }} {{ e.nomeEstrutura }} - {{ e.nomeMaquina }} SN:{{ e.serial }}"
          }
          {% if forloop.revcounter != 1 %}
            ,
          {% endif %}
        {% endfor %}
      ]
}]
